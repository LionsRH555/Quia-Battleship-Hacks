var allItems=window.baDataset;var data="";if(allItems===undefined){alert("There was an error with your request. Please try again later.\nError Code 46: No answers found.");console.log("error46: no answers found.")}try{for(var i=0;i<allItems.questionSet.length;i++){data+=allItems.questionSet[i].questionText+" ";for(var j=0;j<allItems.questionSet[i].answerChoices.length;j++){if(allItems.questionSet[i].answerChoices[j].isCorrect===true){data+="<strong>"+allItems.questionSet[i].answerChoices[j].answerChoiceText+"</strong><br>";}}}var b=document.createElement("p");b.innerHTML="<h1>Answers:</h1><h3><a href='https://github.com/LionsRH555'>Made by LionsRH555</a></h3>"+data;if(window.sessionStorage.getItem("cheatcode")!=="qwerty"){document.body.appendChild(b);alert("Answers are now at the bottom of the page!");window.sessionStorage.setItem("cheatcode","qwerty");}else{alert("Answers are already on the bottom of the page.");}}catch(error){if(confirm("An error occured. Would you like to know the error?")===true){alert(error);}console.log(error);console.log("2 errors occured")}
