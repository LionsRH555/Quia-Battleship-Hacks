var allItems=window.baDataset;var data="";for(var i=0;i<allItems.questionSet.length;i++){data+=allItems.questionSet[i].questionText+" ";for(var j=0;j<allItems.questionSet[i].answerChoices.length;j++){if(allItems.questionSet[i].answerChoices[j].isCorrect===true){data+=allItems.questionSet[i].answerChoices[j].answerChoiceText+"<br>";}}}var b=document.createElement("p");b.innerHTML="Answers:<br>"+data;document.body.appendChild(b);alert("Answers are now at the bottom of the page!");
